# PostgreSQL Email Vault Configuration
# Copy this file to .env and update with your database credentials

# PostgreSQL database host
VAULT_DB_HOST=localhost

# PostgreSQL database port
VAULT_DB_PORT=5432

# PostgreSQL database name
VAULT_DB_NAME=emailvault

# PostgreSQL database username
VAULT_DB_USER=your_username

# PostgreSQL database password
VAULT_DB_PASS=your_password